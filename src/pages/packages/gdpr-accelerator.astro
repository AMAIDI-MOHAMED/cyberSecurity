---
import Layout from '@/layouts/Layout.astro';
import HeroSection from '@/components/sections/HeroSection.astro';
import CTA from '@/components/sections/CTA.astro';

const title = "GDPR Compliance Accelerator";
const currentPath = Astro.url.pathname;
const lang = currentPath.startsWith('/fi') ? 'fi' : 'en';

const heroContent = {
  title: "GDPR Compliance Accelerator",
  subtitle: "Accelerate your GDPR compliance with our comprehensive privacy program designed for modern businesses.",
  description: "Complete GDPR compliance solution including privacy impact assessments, data mapping, policy development, and ongoing privacy management to ensure full regulatory compliance.",
  primaryCTA: { text: "Start GDPR Program", href: "/contact?package=gdpr-accelerator" },
  secondaryCTA: { text: "Download GDPR Guide", href: "/downloads/gdpr-compliance-guide.pdf" }
};

const gdprPrinciples = [
  {
    principle: "Lawfulness, Fairness & Transparency",
    description: "Processing must be lawful, fair, and transparent to the data subject",
    implementation: [
      "Legal basis identification and documentation",
      "Privacy notices and transparency measures",
      "Fair processing assessments",
      "Data subject communication protocols"
    ]
  },
  {
    principle: "Purpose Limitation",
    description: "Data must be collected for specified, explicit, and legitimate purposes",
    implementation: [
      "Purpose specification documentation",
      "Compatible use assessments",
      "Processing purpose restrictions",
      "Regular purpose review processes"
    ]
  },
  {
    principle: "Data Minimisation",
    description: "Data processing must be adequate, relevant, and limited to necessary purposes",
    implementation: [
      "Data minimization assessments",
      "Data collection reviews",
      "Processing necessity evaluations",
      "Regular data audits"
    ]
  },
  {
    principle: "Accuracy",
    description: "Personal data must be accurate and kept up to date",
    implementation: [
      "Data accuracy procedures",
      "Update and correction processes",
      "Data quality monitoring",
      "Rectification mechanisms"
    ]
  },
  {
    principle: "Storage Limitation",
    description: "Data must not be kept longer than necessary",
    implementation: [
      "Data retention schedules",
      "Automated deletion processes",
      "Regular data reviews",
      "Secure disposal procedures"
    ]
  },
  {
    principle: "Integrity & Confidentiality",
    description: "Data must be processed securely with appropriate technical and organizational measures",
    implementation: [
      "Security risk assessments",
      "Technical safeguards implementation",
      "Organizational security measures",
      "Incident response procedures"
    ]
  },
  {
    principle: "Accountability",
    description: "Controllers must demonstrate compliance with GDPR principles",
    implementation: [
      "Compliance documentation",
      "Privacy governance framework",
      "Regular compliance audits",
      "Accountability demonstrations"
    ]
  }
];

const implementationRoadmap = [
  {
    phase: "Phase 1: Foundation & Assessment",
    duration: "Month 1-2",
    objectives: [
      "Establish privacy governance framework",
      "Conduct comprehensive data audit",
      "Identify legal basis for processing",
      "Assess current compliance gaps"
    ],
    deliverables: [
      "Privacy governance charter",
      "Data mapping inventory",
      "Legal basis register",
      "Gap analysis report",
      "Privacy impact assessment template"
    ]
  },
  {
    phase: "Phase 2: Policy & Procedure Development",
    duration: "Month 2-3",
    objectives: [
      "Develop GDPR-compliant policies",
      "Create operational procedures",
      "Establish data subject rights processes",
      "Design privacy training programs"
    ],
    deliverables: [
      "Privacy policy suite",
      "Data processing procedures",
      "Data subject rights processes",
      "Privacy training materials",
      "Vendor assessment framework"
    ]
  },
  {
    phase: "Phase 3: Technical Implementation",
    duration: "Month 3-4",
    objectives: [
      "Implement privacy by design",
      "Deploy technical safeguards",
      "Establish monitoring systems",
      "Configure data protection tools"
    ],
    deliverables: [
      "Privacy by design guidelines",
      "Technical security controls",
      "Data monitoring systems",
      "Consent management platform",
      "Data breach response tools"
    ]
  },
  {
    phase: "Phase 4: Testing & Validation",
    duration: "Month 4-5",
    objectives: [
      "Test data subject rights processes",
      "Validate privacy controls",
      "Conduct compliance assessments",
      "Prepare for regulatory readiness"
    ],
    deliverables: [
      "Process testing reports",
      "Control validation results",
      "Compliance assessment",
      "Regulatory readiness review",
      "Continuous improvement plan"
    ]
  }
];

const dataSubjectRights = [
  {
    right: "Right of Access",
    description: "Individuals can request access to their personal data",
    response_time: "1 month",
    implementation: "Automated data export tools, identity verification, comprehensive data search"
  },
  {
    right: "Right to Rectification",
    description: "Individuals can request correction of inaccurate data",
    response_time: "1 month",
    implementation: "Data correction workflows, audit trails, downstream system updates"
  },
  {
    right: "Right to Erasure",
    description: "Individuals can request deletion of their data",
    response_time: "1 month",
    implementation: "Secure deletion procedures, backup management, third-party notifications"
  },
  {
    right: "Right to Restrict Processing",
    description: "Individuals can request limitation of data processing",
    response_time: "1 month",
    implementation: "Processing restriction flags, system controls, staff notifications"
  },
  {
    right: "Right to Data Portability",
    description: "Individuals can request data in a structured format",
    response_time: "1 month",
    implementation: "Standardized export formats, secure transfer mechanisms, validation processes"
  },
  {
    right: "Right to Object",
    description: "Individuals can object to certain types of processing",
    response_time: "1 month",
    implementation: "Objection handling processes, legitimate interest assessments, processing cessation"
  }
];

const specialCategories = [
  {
    category: "Health Data",
    considerations: "Medical records, health assessments, wellness data",
    safeguards: "Enhanced security, explicit consent, medical professional oversight"
  },
  {
    category: "Biometric Data",
    considerations: "Fingerprints, facial recognition, voice patterns",
    safeguards: "Specific identification purposes, technical safeguards, access restrictions"
  },
  {
    category: "Financial Data",
    considerations: "Banking details, credit scores, payment information",
    safeguards: "Encryption, access controls, fraud prevention measures"
  },
  {
    category: "Location Data",
    considerations: "GPS coordinates, tracking data, geolocation",
    safeguards: "Purpose specification, user consent, anonymization techniques"
  }
];

const pricing = {
  basePrice: "€45,000",
  note: "Pricing varies based on organization size, data complexity, and processing activities",
  tiers: [
    {
      name: "Startup Package",
      price: "€25,000",
      suitable: "Small companies (up to 50 employees)",
      includes: [
        "Basic data mapping",
        "Essential policies and procedures",
        "Privacy notice templates",
        "Data subject rights setup",
        "3 months support"
      ]
    },
    {
      name: "Business Package",
      price: "€45,000",
      suitable: "Medium companies (50-250 employees)",
      includes: [
        "Comprehensive data mapping",
        "Full policy suite",
        "Privacy impact assessments",
        "Training programs",
        "Technical implementation support",
        "6 months support"
      ]
    },
    {
      name: "Enterprise Package",
      price: "€85,000",
      suitable: "Large organizations (250+ employees)",
      includes: [
        "Complex data mapping and modeling",
        "Advanced privacy controls",
        "Multi-jurisdiction compliance",
        "Executive training",
        "Ongoing compliance monitoring",
        "12 months support",
        "Annual compliance review"
      ]
    }
  ]
};
---

<Layout title={title}>
  <HeroSection content={heroContent} />
  
  <!-- GDPR Overview -->
  <section class="py-16 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-slate-900 mb-4">Why GDPR Compliance Matters More Than Ever</h2>
        <p class="text-lg text-slate-600 max-w-4xl mx-auto">
          With over €1.6 billion in fines issued since GDPR came into force, privacy compliance is not optional. 
          Our accelerator program helps you achieve comprehensive GDPR compliance while building customer trust 
          and competitive advantage.
        </p>
      </div>
      
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6">
        <div class="bg-red-50 border-l-4 border-red-400 p-6">
          <div class="flex items-center mb-2">
            <svg class="w-6 h-6 text-red-600 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1"></path>
            </svg>
            <h3 class="text-lg font-semibold text-red-900">High Fines</h3>
          </div>
          <p class="text-red-700">Up to 4% of annual revenue or €20M - whichever is higher</p>
        </div>
        
        <div class="bg-orange-50 border-l-4 border-orange-400 p-6">
          <div class="flex items-center mb-2">
            <svg class="w-6 h-6 text-orange-600 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
            </svg>
            <h3 class="text-lg font-semibold text-orange-900">Reputational Risk</h3>
          </div>
          <p class="text-orange-700">Data breaches and non-compliance damage customer trust</p>
        </div>
        
        <div class="bg-yellow-50 border-l-4 border-yellow-400 p-6">
          <div class="flex items-center mb-2">
            <svg class="w-6 h-6 text-yellow-600 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.414-4.414a2 2 0 010 2.828L9.828 17.657a2 2 0 01-2.828 0L3.172 13.828a2 2 0 010-2.828L9.757 4.414a2 2 0 012.828 0z"></path>
            </svg>
            <h3 class="text-lg font-semibold text-yellow-900">Business Value</h3>
          </div>
          <p class="text-yellow-700">Compliance creates competitive advantage and enables growth</p>
        </div>
        
        <div class="bg-teal-50 border-l-4 border-teal-400 p-6">
          <div class="flex items-center mb-2">
            <svg class="w-6 h-6 text-teal-600 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"></path>
            </svg>
            <h3 class="text-lg font-semibold text-teal-900">Customer Trust</h3>
          </div>
          <p class="text-teal-700">Strong privacy practices build lasting customer relationships</p>
        </div>
      </div>
    </div>
  </section>

  <!-- GDPR Principles -->
  <section class="py-16 bg-slate-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-slate-900 mb-4">Seven Principles of GDPR</h2>
        <p class="text-lg text-slate-600 max-w-3xl mx-auto">
          Our accelerator program ensures your organization fully implements all seven GDPR principles 
          with practical guidance and proven frameworks.
        </p>
      </div>
      
      <div class="grid grid-cols-1 lg:grid-cols-2 gap-8">
        {gdprPrinciples.map((principle, index) => (
          <div class="bg-white rounded-lg shadow-lg border border-slate-200 p-6">
            <div class="flex items-start mb-4">
              <span class="bg-teal-600 text-white text-sm font-bold px-3 py-1 rounded-full mr-3 mt-1">
                {index + 1}
              </span>
              <div>
                <h3 class="text-lg font-semibold text-slate-900 mb-2">{principle.principle}</h3>
                <p class="text-slate-600 mb-4">{principle.description}</p>
              </div>
            </div>
            
            <div>
              <h4 class="text-sm font-semibold text-slate-900 mb-3">Implementation Approach:</h4>
              <ul class="space-y-2">
                {principle.implementation.map(item => (
                  <li class="flex items-start">
                    <svg class="w-4 h-4 text-teal-600 mr-2 mt-1 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                      <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                    </svg>
                    <span class="text-sm text-slate-700">{item}</span>
                  </li>
                ))}
              </ul>
            </div>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Data Subject Rights -->
  <section class="py-16 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-slate-900 mb-4">Data Subject Rights Implementation</h2>
        <p class="text-lg text-slate-600 max-w-3xl mx-auto">
          Comprehensive implementation of all data subject rights with automated workflows and response mechanisms.
        </p>
      </div>
      
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
        {dataSubjectRights.map(right => (
          <div class="bg-slate-50 rounded-lg p-6">
            <div class="flex items-center justify-between mb-3">
              <h3 class="text-lg font-semibold text-slate-900">{right.right}</h3>
              <span class="bg-teal-100 text-teal-800 text-xs px-2 py-1 rounded-full">
                {right.response_time}
              </span>
            </div>
            <p class="text-slate-600 mb-4">{right.description}</p>
            <div>
              <h4 class="text-sm font-semibold text-slate-900 mb-2">Implementation:</h4>
              <p class="text-sm text-slate-700">{right.implementation}</p>
            </div>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Implementation Roadmap -->
  <section class="py-16 bg-slate-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-slate-900 mb-4">5-Month Implementation Roadmap</h2>
        <p class="text-lg text-slate-600 max-w-3xl mx-auto">
          Structured approach to achieve comprehensive GDPR compliance with clear phases and deliverables.
        </p>
      </div>
      
      <div class="space-y-8">
        {implementationRoadmap.map((phase, index) => (
          <div class="bg-white rounded-lg shadow-lg border border-slate-200 p-8">
            <div class="flex flex-col lg:flex-row lg:items-start lg:justify-between mb-6">
              <div class="lg:flex-1">
                <div class="flex items-center mb-3">
                  <span class="bg-teal-600 text-white text-sm font-bold px-3 py-1 rounded-full mr-3">
                    {phase.phase.split(':')[0]}
                  </span>
                  <span class="text-sm text-slate-500">{phase.duration}</span>
                </div>
                <h3 class="text-xl font-semibold text-slate-900 mb-2">{phase.phase.split(':')[1]}</h3>
              </div>
            </div>
            
            <div class="grid grid-cols-1 lg:grid-cols-2 gap-8">
              <div>
                <h4 class="text-sm font-semibold text-slate-900 mb-3">Objectives:</h4>
                <ul class="space-y-2">
                  {phase.objectives.map(objective => (
                    <li class="flex items-start">
                      <svg class="w-4 h-4 text-teal-600 mr-2 mt-1 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                        <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                      </svg>
                      <span class="text-sm text-slate-700">{objective}</span>
                    </li>
                  ))}
                </ul>
              </div>
              
              <div>
                <h4 class="text-sm font-semibold text-slate-900 mb-3">Deliverables:</h4>
                <ul class="space-y-2">
                  {phase.deliverables.map(deliverable => (
                    <li class="flex items-start">
                      <svg class="w-4 h-4 text-orange-500 mr-2 mt-1 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                        <path fill-rule="evenodd" d="M3 4a1 1 0 011-1h12a1 1 0 011 1v2a1 1 0 01-1 1H4a1 1 0 01-1-1V4zM3 10a1 1 0 011-1h6a1 1 0 011 1v6a1 1 0 01-1 1H4a1 1 0 01-1-1v-6zM14 9a1 1 0 00-1 1v6a1 1 0 001 1h2a1 1 0 001-1v-6a1 1 0 00-1-1h-2z" clip-rule="evenodd"></path>
                      </svg>
                      <span class="text-sm text-slate-700">{deliverable}</span>
                    </li>
                  ))}
                </ul>
              </div>
            </div>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Special Categories -->
  <section class="py-16 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-slate-900 mb-4">Special Category Data Handling</h2>
        <p class="text-lg text-slate-600 max-w-3xl mx-auto">
          Expert guidance on handling sensitive personal data with enhanced protections and compliance requirements.
        </p>
      </div>
      
      <div class="grid grid-cols-1 md:grid-cols-2 gap-8">
        {specialCategories.map(category => (
          <div class="bg-slate-50 rounded-lg p-6">
            <h3 class="text-lg font-semibold text-slate-900 mb-3">{category.category}</h3>
            <p class="text-slate-600 mb-4">{category.considerations}</p>
            <div>
              <h4 class="text-sm font-semibold text-slate-900 mb-2">Required Safeguards:</h4>
              <p class="text-sm text-slate-700">{category.safeguards}</p>
            </div>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Pricing Packages -->
  <section class="py-16 bg-slate-50">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-slate-900 mb-4">GDPR Compliance Packages</h2>
        <p class="text-lg text-slate-600 max-w-3xl mx-auto">
          Choose the package that best fits your organization size and compliance requirements.
        </p>
      </div>
      
      <div class="grid grid-cols-1 md:grid-cols-3 gap-8">
        {pricing.tiers.map((tier, index) => (
          <div class={`rounded-lg shadow-lg p-8 ${index === 1 ? 'bg-teal-50 border-2 border-teal-200 transform scale-105' : 'bg-white border border-slate-200'}`}>
            <div class="text-center mb-6">
              <h3 class="text-xl font-semibold text-slate-900 mb-2">{tier.name}</h3>
              <div class="mb-4">
                <span class="text-3xl font-bold text-teal-600">{tier.price}</span>
              </div>
              <p class="text-sm text-slate-600">{tier.suitable}</p>
            </div>
            
            <ul class="space-y-3 mb-8">
              {tier.includes.map(item => (
                <li class="flex items-start">
                  <svg class="w-4 h-4 text-teal-600 mr-2 mt-1 flex-shrink-0" fill="currentColor" viewBox="0 0 20 20">
                    <path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path>
                  </svg>
                  <span class="text-sm text-slate-700">{item}</span>
                </li>
              ))}
            </ul>
            
            <a href={`/contact?package=gdpr-${tier.name.toLowerCase().replace(' ', '-')}`} 
               class={`block w-full text-center py-3 px-4 rounded-md font-medium ${
                 index === 1 
                   ? 'bg-teal-600 text-white hover:bg-teal-700' 
                   : 'bg-slate-100 text-slate-900 hover:bg-slate-200'
               } transition-colors`}>
              Get Started
            </a>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- Compliance Benefits -->
  <section class="py-16 bg-white">
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-12">
        <h2 class="text-3xl font-bold text-slate-900 mb-4">Beyond Compliance: Business Benefits</h2>
        <p class="text-lg text-slate-600 max-w-3xl mx-auto">
          GDPR compliance delivers tangible business value beyond regulatory requirements.
        </p>
      </div>
      
      <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6">
        <div class="text-center">
          <div class="bg-teal-100 w-16 h-16 rounded-full flex items-center justify-center mx-auto mb-4">
            <svg class="w-8 h-8 text-teal-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.414-4.414a2 2 0 010 2.828L9.828 17.657a2 2 0 01-2.828 0L3.172 13.828a2 2 0 010-2.828L9.757 4.414a2 2 0 012.828 0z"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-slate-900 mb-2">Enhanced Trust</h3>
          <p class="text-slate-600">Build stronger customer relationships through transparent privacy practices</p>
        </div>
        
        <div class="text-center">
          <div class="bg-teal-100 w-16 h-16 rounded-full flex items-center justify-center mx-auto mb-4">
            <svg class="w-8 h-8 text-teal-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-slate-900 mb-2">Competitive Edge</h3>
          <p class="text-slate-600">Differentiate from competitors with strong privacy credentials</p>
        </div>
        
        <div class="text-center">
          <div class="bg-teal-100 w-16 h-16 rounded-full flex items-center justify-center mx-auto mb-4">
            <svg class="w-8 h-8 text-teal-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"></path>
            </svg>
          </div>
          <h3 class="text-lg font-semibold text-slate-900 mb-2">Risk Reduction</h3>
          <p class="text-slate-600">Minimize data breach risks and associated costs</p>
        </div>
        
        <div class="text-center">
