---
import Layout from '@/layouts/Layout.astro';
// Temporarily disabled obsolete page content to resolve build errors.
const title = "Webinars & Events";
---
<Layout title={title} description="Redirecting">
  <section class="py-24">
    <div class="max-w-3xl mx-auto px-4 text-center">
      <h1 class="text-3xl font-bold mb-4">Page Moved</h1>
      <p class="text-slate-600 mb-6">Webinars & events have moved to the new <a href="/webinars" class="text-teal-700 underline">/webinars</a> section.</p>
      <a href="/webinars" class="inline-block bg-teal-800 text-white px-6 py-3 rounded-lg">Go to Webinars</a>
    </div>
  </section>
</Layout>

