---
import Layout from "../../layouts/Layout.astro";
const pageTitle = "License Options";
---
<Layout title={pageTitle}>
  <section class="py-20 bg-gradient-to-b from-teal-50 to-white">
    <div class="mx-auto max-w-4xl px-4 sm:px-6 lg:px-8 text-center">
      <h1 class="text-3xl sm:text-4xl font-bold tracking-tight text-slate-900 mb-6">{pageTitle}</h1>
      <p class="text-lg text-slate-600 max-w-2xl mx-auto">Content coming soon.</p>
    </div>
  </section>
</Layout>
